#user  nobody; # Windows # Leave commented out.
#user  nginx; # Linux
#worker_processes auto; # Linux
#worker_processes 1; # Windows

# Windows
#error_log logs/error.log;
#error_log logs/error.log debug;
#error_log logs/error.log notice; # Default
#error_log logs/error.log info;

# Linux
#error_log  log/nginx/error.log;
#error_log  log/nginx/error.log debug;
#error_log  /nginx/error.log notice; # Default
#error_log  /nginx/error.log info;

#pid        logs/nginx.pid; # Windows
#pid        run/nginx.pid; # Linux

events {
    worker_connections  1024;
}

http {
    include       mime.types;
    default_type  application/octet-stream;

    #log_format  main  '$remote_addr - $remote_user [$time_local] "$request" '
    #                  '$status $body_bytes_sent "$http_referer" '
    #                  '"$http_user_agent" "$http_x_forwarded_for"';

    #access_log  log/nginx/access.log; # Linux
    #access_log  logs/access.log; # Windows

    sendfile        on;
    #tcp_nopush     on;

    keepalive_timeout  65;

    #gzip  on;

    include conf.d/*.conf;
}
